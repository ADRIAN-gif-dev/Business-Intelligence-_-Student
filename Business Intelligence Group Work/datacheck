   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "from ucimlrepo import fetch_ucirepo\n",
    "\n",
    "# --- 0. LOAD DATA ---\n",
    "print(\"Loading Adult dataset...\")\n",
    "adult = fetch_ucirepo(id=2)\n",
    "# Combine features and targets into one table\n",
    "df = pd.concat([adult.data.features, adult.data.targets], axis=1)\n",
    "\n",
    "# --- 1. CHECK MISSING VALUES ---\n",
    "print(\"\\n--- 1. MISSING VALUES CHECK ---\")\n",
    "# Standard check (NaN)\n",
    "print(f\"Standard NaNs count: {df.isnull().sum().sum()}\")\n",
    "\n",
    "# The Adult dataset specific check (It uses '?' instead of empty space)\n",
    "print(\"Occurrences of '?':\")\n",
    "print((df == '?').sum())\n",
    "# (If you see numbers here, those are your real missing values)\n",
    "\n",
    "\n",
    "# --- 2. CHECK DUPLICATES ---\n",
    "print(\"\\n--- 2. DUPLICATES CHECK ---\")\n",
    "duplicate_count = df.duplicated().sum()\n",
    "print(f\"Total Duplicate Rows: {duplicate_count}\")\n",
    "# (If this number is > 0, you have duplicates)\n",
    "\n",
    "\n",
    "# --- 3. CHECK INCONSISTENT LABELS ---\n",
    "print(\"\\n--- 3. INCONSISTENT LABELS CHECK ---\")\n",
    "# checking the 'income' column for the famous '.' bug\n",
    "unique_labels = df['income'].unique()\n",
    "print(\"Unique values in 'income' column:\")\n",
    "print(unique_labels)\n",
    "# (If you see both '<=50K' and '<=50K.', you have inconsistent labels)\n"
   ],
